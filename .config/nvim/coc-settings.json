{
  "suggest.noselect": false,
  "diagnostic.displayByAle": false,
  "coc.preferences.rootPatterns": [
    ".git",
    ".root",
    ".hg",
    ".projections.json",
    "package.json",
    "__pycache__",
    ".atsln",
    ".cproj",
    ".env"
  ],
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "args": ["--log-file=/tmp/ccls.log", "-v=1"],
      "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json"],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        },
        "client": {
          "snippetSupport": true
        },
        "index": {
          "onChange": true
        },
        "highlight": { "lsRanges": true }
      }
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "c",
    "c++",
    "scss",
    "css",
    "typescript",
    "python",
    "typescriptreact",
    "json",
    "jsonc",
    "javascriptreact",
    "javascript.tsx",
    "typescript.tsx"
  ],
  "clangd.path": "/home/damndiamond/.config/coc/extensions/coc-clangd-data/install/11.0.0/clangd_11.0.0/bin/clangd",
  "coc.preferences.colorSupport": true,
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "python.formatting.provider": "black",
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "diagnostic.hintSign": " ",
  "eslint.autoFixOnSave": true,
  "stylelintplus.enable": true,
  "stylelintplus.autoFixOnSave": true,
  "css.validate": false,
  "scss.validate": false,
  "wxss.validate": false,
  "less.validate": false,
  "python.jediEnabled": false,
  /* "eslint.options": { */
  /*   "configFile": "/home/damndiamond/.config/nvim/eslintrc.json" */
  /* }, */
  "emmet.preferences": {
    "typescriptreact": "scss",
    "jsx.classAttributeName": "scss",
    "typescript.tsx.classAttributeName": "scss"
  }
}
